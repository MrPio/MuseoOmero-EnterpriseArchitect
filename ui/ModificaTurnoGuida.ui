}

QComboBox::down-arrow {
    image: url(:/icons/expand_more_FILL1_wght600_GRAD200_opsz48_risultato.png);

    width: 26px;
    height: 26px;
margin: 0px 0px 0px 0px
}

QComboBox::drop-down  {
	background-color:rgb(104, 194, 253);
border-radius:6px;
margin:-3px -3px -3px -10px;
    border: 0px; /* This seems to replace the whole arrow of the combo box */

}



/* VERTICAL SCROLLBAR */
 QScrollBar:vertical {
background-color:#fff9be;
	border: none;
    width: 14px;
    margin: 0px 0 0px 0;
	border-radius: 7px;
margin: 0px 0px 0px 0px;

 }

/*  HANDLE BAR VERTICAL */
QScrollBar::handle:vertical {	
	background-color:#68C2FD;
	min-height: 30px;	
	border-radius: 7px;

}
QScrollBar::handle:vertical:hover{	
	background-color:#A6E4FF;
}
QScrollBar::handle:vertical:pressed {	
	background-color: #5AA7D9;
}

/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:vertical {
	background-color: rgb(255, 255, 255);
	border-radius: 7px;
