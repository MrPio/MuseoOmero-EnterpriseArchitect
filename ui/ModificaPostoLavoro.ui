QSpinBox::focus{
background:rgb(255, 255, 255);
border-radius:8px;
border-color:rgb(104, 194, 253);
border-style: solid;
border-width:3;
}


QSpinBox::down-arrow {
    image: url(:/icons/expand_more_FILL1_wght600_GRAD200_opsz48_risultato.png);
background-color:rgb(104, 194, 253);
	width: 26px;
    height: 26px;
	margin: 0px 0px 0px 0px
}
	
QSpinBox::up-arrow {
    image: url(:/icons/keyboard_arrow_up_FILL1_wght600_GRAD200_opsz48_risultato.png);
}

QSpinBox::down-arrow  {
	background-color:rgb(104, 194, 253);
border-bottom-left-radius: 6px;
border-bottom-right-radius: 6px;
	margin:-2px -7px -3px -6px;

    border: 0px; /* This seems to replace the whole arrow of the combo box */
}

QSpinBox::up-arrow  {
	background-color:rgb(104, 194, 253);
border-top-left-radius: 6px;
